{% extends "base" %}

{% block content %}

    <h1>{{ title }}</h1>


    <table>
    {% for i in items %}
        <tr>
            
                <td><a href="{{ title }}/{{ i.link }}">{{ i.title }}</a></td>
                <td id="table-date">{{ i.date }}</td>
            
        </tr>
    {% endfor %}

    </table>

{% endblock content %}
